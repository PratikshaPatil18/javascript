<html>
<head><title>max demo</title></head>
<script type="text/javascript">
  var arr=[10,30,90,68,389];
  var max=0;
  max=arr[0];
  var max2=0
  
  document.write("Original array:"+arr);
 
  for(let i=0;i<=arr.length-1;i++)
  {
   if(arr[i]>max)
   {
	   max2=max;
	   max=arr[i];
   }
 }
  for(let i=0;i<=arr.length-1;i++)
  {
       if(arr[i] > max2 && arr[i] < max)
		  {
		  max2=arr[i];
		  }
  }
  document.write("Max element from array :"+max);
  document.write("<br>2nd Max element from array :"+max2);
</script>
</html>